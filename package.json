{
  "name": "unplugin-injector-version-html",
  "version": "1.0.17",
  "description": "This project demonstrates how to create a custom Webpack plugin that injects a version number into an HTML file during the build process.",
  "scripts": {
    "build": "tsup"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "typesVersions": {
    "*": {
      "webpack": [
        "./dist/webpack.d.ts"
      ],
      "vite": [
        "./dist/vite.d.ts"
      ]
    }
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./webpack": {
      "import": "./dist/webpack.mjs",
      "require": "./dist/webpack.js"
    },
    "./vite": {
      "import": "./dist/vite.mjs",
      "require": "./dist/vite.js"
    }
  },
  "files": [
    "dist",
    "README-en.md"
  ],
  "keywords": [
    "webpack",
    "vite",
    "plugin",
    "html",
    "version",
    "injector"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^22.15.3",
    "tsup": "^8.4.0",
    "vite": "^6.3.5",
    "webpack": "^5.99.7",
    "webpack-sources": "^3.2.3"
  },
  "packageManager生成": "corepack use pnpm@10.10.0",
  "packageManager": "pnpm@10.10.0+sha256.fa0f513aa8191764d2b6b432420788c270f07b4f999099b65bb2010eec702a30"
}
